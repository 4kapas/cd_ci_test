{
  "name": "potree_setup",
  "private": true,
  "version": "0.0.0",
  "license": "MIT",
  "main": "public/electron.js",
  "homepage": ".",
  "description": "Your app description",
  "author": "Your name",
  "build": {
    "appId": "com.my-website.my-app",
    "productName": "MyApp",
    "copyright": "Copyright Â© 2019 ${author}",
    "directories": {
      "buildResources": "assets",
      "output": "dist_electron"
    },
    "extraMetadata": {
      "main": "public/electron.js"
    },
    "files": [
      "dist/**/*",
      "public/electron.js",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "mac": {
      "category": "public.app-category.utilities"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "assets/icons"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "gbuild": "gulp build pack",
    "electron": "electron .",
    "electron:pack": "electron-builder --dir",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none yarn dev\" \"yarn electron\"",
    "electron:builder": "electron-builder",
    "build:for:electron": "tsc --noEmit && cross-env ELECTRON=true vite build",
    "app:build": "yarn build:for:electron && yarn electron:builder",
    "start": "npm run gbuild && npm run dev"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.14.7",
    "@mui/x-data-grid": "^7.28.3",
    "@mui/x-date-pickers": "^6.13.0",
    "@ngageoint/geopackage": "^4.2.5",
    "@types/html2canvas": "^1.0.0",
    "axios": "^1.5.0",
    "better-sqlite3": "^11.1.2",
    "bootstrap": "^5.2.3",
    "circular-json": "^0.5.9",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "density-clustering": "^1.3.0",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.9",
    "electron-is-dev": "^3.0.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "file-saver": "^2.0.5",
    "gulp-cli": "^2.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.7.0",
    "gulp-env": "^0.4.0",
    "html2canvas": "^1.4.1",
    "icp-js": "^1.2.2",
    "icpts": "^0.6.0",
