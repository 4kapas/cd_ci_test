    "react": "^18.2.0",
    "react-bootstrap": "^2.7.2",
    "react-copy-to-clipboard": "^5.1.0",
    "react-datepicker": "^4.17.0",
    "react-dom": "^18.2.0",
    "react-error-overlay": "^6.0.11",
